{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": "off",
    "axe/name-role-value": [
      "default",
      {
        "input-button-name": "off",
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "overflow: clip",
          "overflow-clip-margin"
        ]
      }
    ],
    "axe/parsing": "off",
    "disown-opener": "off",
    "button-type": "off"
  }
}